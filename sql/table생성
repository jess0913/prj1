
# 공지사항 테이블 생성

CREATE TABLE ADMIN.noti 
(
  id NUMBER NOT NULL,
  title VARCHAR2(250),
  description VARCHAR2(4000),
  createDate DATE,
  createUser VARCHAR2(100),
  updateDate DATE,
  updateUser VARCHAR2(100),
  CONSTRAINT PK_noti PRIMARY KEY(id)
)
TABLESPACE BOARD;


# 공지사항 시퀀스 생성
CREATE SEQUENCE ADMIN.id_seq
START WITH 1
INCREMENT BY 1
NOCYCLE
NOCACHE 

# 자료실 테이블 생성
CREATE TABLE ADMIN.CONTENTS (
  CONTENTSID      NUMBER             NOT NULL, 
  FILENAME        VARCHAR2(200)      NOT NULL, 
  FILEURL         VARCHAR2(300)      NOT NULL, 
  CREATEDATE      DATE               NOT NULL, 
  CREATEUSER      VARCHAR2(100)      NOT NULL, 
  UPDATEDATE      DATE               NOT NULL, 
  UPDATEUSER      VARCHAR2(100)      NOT NULL, 
  DESCRIPTION     VARCHAR2(2000)         NULL, 
  CONTENTSNAME    VARCHAR2(100)      NOT NULL
)
TABLESPACE BOARD

# 자료실 시퀀스 생성
CREATE SEQUENCE ADMIN.CONTENTSID_seq
START WITH 1
INCREMENT BY 1
NOCYCLE
NOCACHE 